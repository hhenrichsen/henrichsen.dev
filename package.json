{
  "name": "henrichsen.dev",
  "private": true,
  "scripts": {
    "inst": "bun --cwd packages/plugins install && bun --cwd packages/site install",
    "build": "bun run --cwd packages/site build",
    "dev": "bun --cwd packages/site run dev",
    "test": "bun --cwd packages/plugins run test",
    "test:e2e": "bun --cwd packages/plugins run test:e2e"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
